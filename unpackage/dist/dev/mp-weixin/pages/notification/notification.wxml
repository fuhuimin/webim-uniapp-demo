<view><view class="notification_title"><text>通知</text></view><view data-event-opts="{{[['tap',[['into_friendNot',['$event']]]]]}}" class="address_bottom" bindtap="__e"><image src="/static/images/notification.png"></image><text>加好友申请</text><block wx:if="{{messageNum>0}}"><text class="notification_dot">{{messageNum}}</text></block></view><view data-event-opts="{{[['tap',[['into_groupNot',['$event']]]]]}}" class="address_bottom" bindtap="__e"><image src="/static/images/jiaqun@2x.png"></image><text>加群组申请</text><block wx:if="{{groupInviteNum>0}}"><text class="notification_dot">{{groupInviteNum}}</text></block></view><view class="{{[isIPX?'chatRoom_tab_X':'chatRoom_tab']}}"><view data-event-opts="{{[['tap',[['tab_chat',['$event']]]]]}}" class="tableBar" bindtap="__e"><block wx:if="{{unReadSpotNum>0}}"><view class="{{['em-unread-spot '+(unReadSpotNum=='99+'?'em-unread-spot-litleFont':'')]}}">{{unReadSpotNum}}</view></block><image class="{{[unReadSpotNum>0?'haveSpot':'']}}" src="/static/images/session@2x.png"></image><text>聊天</text></view><view data-event-opts="{{[['tap',[['tab_contact',['$event']]]]]}}" class="tableBar" bindtap="__e"><image src="/static/images/comtacts@2x.png"></image><text>联系人</text></view><view class="tableBar"><block wx:if="{{unReadTotalNotNum>0||unReadSpotNum=='99+'}}"><view class="em-unread-spot">{{unReadTotalNotNum}}</view></block><image class="{{[unReadTotalNotNum>0||unReadSpotNum=='99+'?'haveSpot':'']}}" src="/static/images/noticeActive.png"></image><text class="activeText">通知</text></view><view data-event-opts="{{[['tap',[['tab_setting',['$event']]]]]}}" class="tableBar" bindtap="__e"><image src="/static/images/setting@2x.png"></image><text>设置</text></view></view></view>