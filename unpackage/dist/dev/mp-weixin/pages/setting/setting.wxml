<view><view class="setting_title"><text>设置</text></view><view class="setting_head"><view class="head_pic"><image src="/static/images/theme@2x.png"></image><view><text class="setting_username">{{yourname}}</text><text class="setting_username2">username</text></view></view></view><view class="setting_list"><view data-event-opts="{{[['tap',[['goGeneralSetting',['$event']]]]]}}" class="setting_listContent" bindtap="__e"><text>通用设置</text><image class="list_right" src="/static/images/next.png"></image></view></view><view class="setting_list"><view data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="setting_listContent" bindtap="__e"><text class="setting_redtext">退出登录</text></view></view><view class="{{[isIPX?'chatRoom_tab_X':'chatRoom_tab']}}"><view data-event-opts="{{[['tap',[['tab_chat',['$event']]]]]}}" class="tableBar" bindtap="__e"><block wx:if="{{unReadSpotNum>0||unReadSpotNum=='99+'}}"><view class="{{['em-unread-spot '+(unReadSpotNum=='99+'?'em-unread-spot-litleFont':'')]}}">{{unReadSpotNum}}</view></block><image class="{{[unReadSpotNum>0||unReadSpotNum=='99+'?'haveSpot':'']}}" src="/static/images/session@2x.png"></image><text>聊天</text></view><view data-event-opts="{{[['tap',[['tab_contact',['$event']]]]]}}" class="tableBar" bindtap="__e"><image src="/static/images/comtacts@2x.png"></image><text>联系人</text></view><view data-event-opts="{{[['tap',[['tab_notification',['$event']]]]]}}" class="tableBar" bindtap="__e"><block wx:if="{{unReadTotalNotNum>0}}"><view class="em-unread-spot">{{unReadTotalNotNum}}</view></block><image class="{{[unReadTotalNotNum>0?'haveSpot':'']}}" src="/static/images/notice.png"></image><text>通知</text></view><view class="tableBar"><image src="/static/images/settinghighlight@2x.png"></image><text class="activeText">设置</text></view></view></view>